{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ripple",
  "compatibility_date": "2026-02-07",
  "assets": {
    "directory": "./dist",
    "not_found_handling": "single-page-application"
  },
  "routes": [
    {
      "pattern": "ripple.conduits.space",
      "custom_domain": true,
      "override_existing_dns_record": true
    }
  ]
}
